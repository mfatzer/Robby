#ifndef MOTORDIRECTION_H_
#define MOTORDIRECTION_H_

#ifdef __cplusplus
extern "C" {
#endif

enum MotorDirection
{
	FORWARD,
	BACKWARD,
	STOP
};

#ifdef __cplusplus
} // extern "C"
#endif

#endif /* MOTORDIRECTION_H_ */
