#include "Logger.h"
#include "Arduino.h"


Logger::Logger() {
	// TODO Auto-generated constructor stub

}

Logger::~Logger() {
	// TODO Auto-generated destructor stub
}

void Logger::log(char* info) {
	Serial.print(m_name);
	Serial.print(":");
	Serial.println(info);
}

